{
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "fabric": true
  },
  "extends": "standard",
  "rules": {
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "no-console": "warn"
  },
  "plugins": [
    "html"
  ]
}
